{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-secure-store",  
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow WiseClone to access your Face ID biometric data."
        }
      ]
    ],
    "name": "WiseClone",
    "slug": "WiseClone"
  }
}

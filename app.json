{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#87e95b"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow WiseClone to access your Face ID biometric data."
        }
      ]
    ],
    "name": "WiseClone",
    "slug": "WiseClone",
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "2cf59db1-c3e1-41da-a1f5-c1d742031738"
      }
    },
    "android": {
      "package": "com.eske17.WiseClone"
    }
  }
}
